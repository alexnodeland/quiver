{
  "name": "@quiver/react",
  "version": "0.1.0",
  "description": "React utilities for Quiver modular synthesizer",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "quiver",
    "modular",
    "synthesizer",
    "audio",
    "react",
    "react-flow"
  ],
  "author": "Quiver Contributors",
  "license": "MIT",
  "dependencies": {
    "@quiver/types": "workspace:*"
  },
  "peerDependencies": {
    "@xyflow/react": ">=12.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@xyflow/react": "^12.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
